<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroud"
    tools:context=".MainActivity2">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="407dp"
        android:layout_height="63dp"
        android:background="@drawable/buttonsbackground"
        android:backgroundTint="@color/accent_dark"
        android:foregroundGravity="center"
        android:gravity="center"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:buttonGravity="center_vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.982"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.008"
        app:titleTextColor="@color/accent_dark" />

    <TextView
        android:id="@+id/MainTextShiftHistoryAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="Welcome,uname"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/StartShiftButtonMain"
        android:layout_width="258dp"
        android:layout_height="81dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/buttonsbackground"
        android:backgroundTint="@color/accent"
        android:text="Start Shift"
        android:textAllCaps="false"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.163"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SalaryTextMain"
        app:layout_constraintVertical_bias="0.654" />

    <TextView
        android:id="@+id/EarnedTextMain"
        android:layout_width="159dp"
        android:layout_height="147dp"
        android:layout_marginBottom="424dp"
        android:background="@drawable/buttonsbackground"
        android:backgroundTint="@color/accent_dark"
        android:gravity="center"
        android:text="Earned"
        android:textAlignment="center"
        android:textColor="@color/accent_light"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.912"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/SalaryTextMain"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/HoursTextMain"
        android:layout_width="159dp"
        android:layout_height="147dp"
        android:background="@drawable/buttonsbackground"
        android:backgroundTint="@color/accent_dark"
        android:gravity="center"
        android:text="Hours"
        android:textAlignment="center"
        android:textColor="@color/accent_light"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SalaryTextMain"
        app:layout_constraintVertical_bias="0.096" />

    <TextView
        android:id="@+id/NextWorkdayTextMain"
        android:layout_width="159dp"
        android:layout_height="147dp"
        android:background="@drawable/buttonsbackground"
        android:backgroundTint="@color/accent_dark"
        android:gravity="center"
        android:text="NextWorkday"
        android:textAlignment="center"
        android:textColor="@color/accent_light"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.912"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EarnedTextMain"
        app:layout_constraintVertical_bias="0.094" />

    <Button
        android:id="@+id/CustomShiftButtonMain"
        android:layout_width="96dp"
        android:layout_height="81dp"
        android:background="@drawable/buttonsbackground"
        android:backgroundTint="@color/accent_light"
        android:text="Custom time"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/StartShiftButtonMain"
        app:layout_constraintTop_toTopOf="@+id/StartShiftButtonMain" />

    <TextView
        android:id="@+id/SalaryTextMain"
        android:layout_width="159dp"
        android:layout_height="147dp"
        android:background="@drawable/buttonsbackground"
        android:backgroundTint="@color/accent_dark"
        android:gravity="center"
        android:text="Salary"
        android:textAlignment="center"
        android:textColor="@color/accent_light"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.186" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="8dp"
        android:text="Hours done:"
        android:textColor="@color/accent"
        app:layout_constraintStart_toStartOf="@+id/HoursTextMain"
        app:layout_constraintTop_toTopOf="@+id/HoursTextMain" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:text="Earned this month:"
        android:textColor="@color/accent"
        app:layout_constraintStart_toStartOf="@+id/EarnedTextMain"
        app:layout_constraintTop_toTopOf="@+id/EarnedTextMain" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="56dp"
        android:text="Salary:"
        android:textColor="@color/accent"
        app:layout_constraintEnd_toEndOf="@+id/SalaryTextMain"
        app:layout_constraintTop_toTopOf="@+id/SalaryTextMain" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="12dp"
        android:text="Next workday:"
        android:textColor="@color/accent"
        app:layout_constraintStart_toStartOf="@+id/NextWorkdayTextMain"
        app:layout_constraintTop_toTopOf="@+id/NextWorkdayTextMain" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/tabslayout"
        android:layout_width="377dp"
        android:layout_height="66dp"
        android:background="@drawable/buttonsbackground"
        app:backgroundTint="@color/accent_dark"
        app:itemIconTint="@drawable/selector"
        app:itemTextColor="@color/accent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.975"
        app:menu="@menu/bottom_nav_menu" />

</androidx.constraintlayout.widget.ConstraintLayout>